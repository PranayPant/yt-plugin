{

  "description": "Download video or playlist in either mp3 or mp4 format form YouTube",
  "manifest_version": 2,
  "name": "KeepTube",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/watch?v=*", 
                  "https://www.youtube.com/playlist?list=*"
                 ],
      "js": ["download.js"]
    }
  ]

}